<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <%- include ('../partials/head.ejs') %>
  </head>
  <body>
    <h1>Index Item Page</h1>
    <ul>
      <% for (let i = 0; i < items.length; i++) { %>
      <li>
        <a href="/items/<%=items[i].id%>">
        <%=items[i].name%>
        </a>
      </li>
     <% } %>
    </ul>
    <nav>
      <a href="/items/new">New Item</a>
    </nav>
  </body>
</html>
